<!-- About Us Page - BE. Brand Aligned -->
@{
    ViewData["Title"] = "About Us";
}

<!-- Hero Section -->
<section class="about-hero d-flex align-items-center justify-content-center text-center text-white font-primary" style="height: 100vh; background: linear-gradient(rgba(0,0,0,0.7), rgba(0,0,0,0.4)), url('/images/about/about-hero.webp') center center / cover no-repeat; background-attachment: fixed;">
    <div class="container">
        <h1 class="display-2 fw-bold mb-4 animate__animated animate__fadeIn" style="font-family: var(--font-secondary); color: var(--orange);">We Make You Who You BE.</h1>
